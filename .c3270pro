c3270.DefaultFgBg: true
c3270.Oversize: 162x60
! Use the 'mine' keymap, defined below
c3270.keymap: mine
! Definition of the 'mine' keymap
c3270.keymap.mine: \
 <Key>PPAGE: PF(7)\n\
 <Key>NPAGE: PF(8)\n\
 <Key>U+007F: Backspace()\n\
 Ctrl<Key>z: EraseEOF()\n\
 Ctrl<Key>q: Attn()\n\
 <Key>U+00A1: Home() String("swap 1") Enter()\n\
 <Key>U+2122: Home() String("swap 2") Enter()\n\
 <Key>U+00A3: Home() String("swap 3") Enter()\n\
 <Key>U+00A2: Home() String("swap 4") Enter()\n\
 <Key>U+221E: Home() String("swap 5") Enter()\n\
 <Key>U+00A7: Home() String("swap 6") Enter()\n\
 <Key>U+00B6: Home() String("swap 7") Enter()\n\
